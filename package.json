{
    "name": "dust-compiler",
    "version": "0.2.0",
    "author": "James Young <jmeyoung@gmail.com> (http://jamsyoung.com)",
    "description": "Dust.js template watcher/compiler with support for notifications on OS X, Linux, and even Windows",
    "preferGlobal": true,
    "main": "dust-compiler.js",
    "bin": {
        "dust-compiler": "./dust-compiler.js"
    },
    "repository": {
        "type": "git",
        "url": "https://github.com/jamsyoung/dust-compiler"
    },
    "bugs": {
        "url": "https://github.com/jamsyoung/dust-compiler/issues",
        "email": "jmeyoung@gmail.com"
    },
    "man": "./man/man1/dust-compiler.1",
    "scripts": {
        "build-man": "node_modules/.bin/marked-man src/man/man1/dust-compiler.1.md > man/man1/dust-compiler.1",
        "test": "grunt test",
        "travis-cov": {
            "threshold": 100
        }
    },
    "dependencies": {
        "colors": "0.6.2",
        "dustjs-linkedin": "2.0.3",
        "growl": "1.7.0",
        "log-notify": "0.1.1",
        "mkdirp": "0.3.5",
        "optimist": "0.6.0",
        "terminal-notifier": "0.1.2",
        "watch": "0.8.0",
        "wrench": "1.5.1"
    },
    "devDependencies": {
        "blanket": "1.1.5",
        "chai": "1.7.2",
        "debug": "0.7.2",
        "grunt": "0.4.1",
        "grunt-complexity": "0.1.3",
        "grunt-contrib-clean": "0.5.0",
        "grunt-contrib-jshint": "0.6.3",
        "grunt-mocha-test": "0.6.3",
        "marked-man": "0.0.2",
        "mocha": "1.12.0",
        "travis-cov": "0.2.4"
    },
    "engines": {
        "node": ">=0.10.13",
        "npm": ">=1.3.2"
    },
    "keywords": [
        "compiler",
        "dust compiler",
        "dust",
        "dust.js",
        "dustjs",
        "dustjs-linkedin",
        "notification center",
        "notify-send",
        "terminal-notifier"
    ],
    "license": {
        "type": "MIT",
        "url": "https://github.com/jamsyoung/dust-compiler/blob/master/LICENSE.md"
    }
}
